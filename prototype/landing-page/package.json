{
    "name": "climateguide",
    "homepage": "https://maggievu.github.io/climateguide/",
    "version": "0.1.0",
    "dependencies": {
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "gh-pages": "^2.0.1"
},
    "devDependencies": {
        "concurrently": "^4.0.1"
    },
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "start": "nodemon server.js",
        "deploy": "cd client && npm run deploy"
    }
}
